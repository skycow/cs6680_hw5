function [ labelIm, num ] = FindComponentLabels( im, se )
%FindComponentLabels label connected objects in a binary image
%   Detailed explanation goes here


[fx, fy] = find(im,1);

Xo = zeros(size(im));

Xo(fx, fy) = 1;

Xi = imdilate(Xo, se);

while isequal(Xo, Xi)
    Xo = Xi;
    Xi = imdilate(Xo, se);
end



end

